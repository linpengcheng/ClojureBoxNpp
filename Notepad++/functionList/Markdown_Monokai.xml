<NotepadPlus>
	<functionList>
		<!-- ==== [ Markdown_Monokai ] -->
      <parser
        displayName="Markdown_Monokai"
        id         ="Markdown_Monokai"
        commentExpr=""
      >
        <function
          mainExpr="(?x)((?:^|\n)\s*[#]+\s+(.*?)(\r*|\n*)$)
                       |((?:^|\n)\s*[\r\n]+\s*[-]{3,}\s*[\r\n]+\s*(\r*|\n*)$)"
        />
      </parser>
	</functionList>
</NotepadPlus>